[% USE raw %]
[% USE Asset %]
[% USE KohaDates %]
[% SET footerjs = 1 %]
[% TRY %]
    [% lang = "$plugin_dir/i18n/$lang_dialect"_".inc" %]
    [% PROCESS "$lang" %]
[% CATCH %]
    [% TRY %]
        [% lang = "$plugin_dir/i18n/$lang_all"_".inc" %]
        [% PROCESS "$lang" %]
    [% CATCH %]
        [% PROCESS "$plugin_dir/i18n/default.inc" %]
    [% END %]
[% END %]
[% INCLUDE 'doc-head-open.inc' %]

<title>Koha: [% MSG.module_name %]: [% MSG.receive_order %]</title>
[% INCLUDE 'doc-head-close.inc' %]
<body>
[% INCLUDE 'header.inc' %]
[% INCLUDE 'cat-search.inc' %]

<div id="breadcrumbs"><a href="/cgi-bin/koha/mainpage.pl">[% MSG.home %]</a> &rsaquo; <a href="/cgi-bin/koha/plugins/plugins-home.pl">[% MSG.plugins %]</a> &rsaquo;  [% MSG.module_name %] &rsaquo; [% MSG.receive_order %]</div>

<div class="main container-fluid">
  <div class="row">
    <div class="col-sm-10 col-sm-push-2">
      <main>
        <h3>Koha:  [% MSG.module_name %]: [% MSG.send_test_data %]</h3>
        <form method="POST">
          <dl>
            <dt><label for="OrderInvoiceNumber">OrderInvoiceNumber</label></dt>
            <dd><input name="OrderInvoiceNumber" value="DF2323SD" type="text" /></dd>
            <dt><label for="ContinueOrderingReturnURL">ContinueOrderingReturnURL</label></dt>
            <dd><input name="ContinueOrderingReturnURL" value="https://adlibris.se/" type="text" /></dd>
            <dt><label for="CustomerNumber">CustomerNumber</label></dt>
            <dd><input name="CustomerNumber" value="A123123" /></dd>
          </dl>
        </form>
      </main>
    </div>
    <div class="col-sm-2 col-sm-pull-10">
      <aside>
        [% INCLUDE 'acquisitions-menu.inc' %]
      </aside>
    </div> <!-- /.col-sm-2.col-sm-pull-10 -->
  </div>
</div>
[% INCLUDE 'intranet-bottom.inc' %]
