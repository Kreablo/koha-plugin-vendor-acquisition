[% TRY %]
    [% lang_file = "$plugin_dir/i18n/$lang_dialect"_".inc" %]
    [% PROCESS "$lang_file" %]
[% CATCH %]
    [% TRY %]
        [% lang_file = "$plugin_dir/i18n/$lang_all"_".inc" %]
        [% PROCESS "$lang_file" %]
    [% CATCH %]
        [% PROCESS "$plugin_dir/i18n/default.inc" %]
    [% END %]
[% END %]
[% IF estimated_delivery_date %]
[% MSG.estimated_delivery_date %]: [% estimated_delivery_date %]
[% END %]

[% IF note %]
[% MSG.note %]: [% note %]
[% END %]